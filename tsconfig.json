{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "lib": ["esnext", "dom"],
    "plugins": [{ "name": "@vuedx/typescript-plugin-vue" }],
    "baseUrl": "./",
    "paths": {
    "@/*": [
        "./src/*"
      ],
    },
  },
  "include": ["src", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"]
}
